<html>
<head>
<style>
body {
  background-image: url('<%= asset_path 'gnappy.jpg' %>');
  
}
</style>
</head>
<body>



<h1>Log in</h1>


<div class="container">
<%= form_with model: @user, url: login_path, method: :post  do |f| %>
  <%= f.label :username %><br>
  <%= f.text_field :username %><br>
  <%= f.label :password %><br>
  <%= f.password_field :password %> <br>
  <%= f.submit "Submit" %> <br>

<% end %>
</div>

</body>
</html>